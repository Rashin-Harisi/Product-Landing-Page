*{
	box-sizing: border-box;
}
body{
	background-color: rgb(229, 225, 227);
	height: 100%;
}

header img{
	width: 300px;
	height: 40px;
	
}
.header{
	width: 100%;
	height: 60px;
	padding-bottom: 40px;
	position: sticky;
	top: 0; 
	z-index: 1;
}
header{
	display: flex;
	justify-content: space-between;
	height: 70px;
	padding-top: 20px;
	background-color: rgb(229, 225, 227)
}

.navbar{
	width: 700px;
	height: 100%;
	display: flex;
	flex-direction: row;
	justify-content: space-evenly;
	margin : auto 0;
	font-family: Sans-serif;
	font-size: 20px;
}
.getinfo{
	display: flex;
	flex-direction : column;
	align-items: center;
	
}
.getinfo h1{
	text-align: center;
}
#emailadd{
	margin-bottom: 15px;
	height: 20px;
	
}
#getstart{
	font-weight: bold;
	background-color: orange;
	height: 30px;
	border: none;
}
.advantages{
	display: grid;
	grid-template-columns: 100px 900px;
	align-items: center;
	padding-left: 15%;
	column-gap: 30px;
	margin-top: 40px;
}
.fa-solid{
	font-size: 50px;
	color: rgb(245, 163, 10);
}
.advantages h3{
	font-size: 24px;
	margin-bottom: 0;
}
.advantages p{
	margin-top: 0;
	font-size: 18px;
}
.advantages div{
	margin: 20px 0;
}
.video iframe{
	position : relative;
	left: 30%;
	width: 600px;
	height:300px;
	margin-bottom: 80px;
	margin-top: 20px;
}
.selection{
	display: flex;
	flex-direction: row;
	justify-content: space-evenly;
	width: 1100px;
	position: relative;
	left:10%;
	margin-bottom: 70px;
}
.subselection{
	border: 0.5px solid grey;
	width: 250px;
	height: 300px;
	display: flex;
	flex-direction: column;
	align-items: center;
	line-height: 2;
}
div .head{
	background-color: grey;
	width: 249px;
	height: 40px;
	text-align: center;
	padding-top: 5px;
	
}
.price{
	font-weight: bold;
	font-size: 24px;
}
.subselection input[type='button']{
	border: none;
	background-color: orange;
	height: 30px;
	margin: 30px 0;
}
footer{
	background-color: #C9C9CB;
	width:1000px;
	height: 70px;
	position: relative;
	left: 15%;
	padding: 10px 0;
	display: flex;
	flex-direction: column;
	align-items: flex-end;
	padding-right: 20px;
	
	
}
.bottomnavbar{
	width: 200px;
	display: flex;
	flex-direction: row;
	justify-content: space-between;
}
.navbare{
	text-decoration: none;
	color: black;
}